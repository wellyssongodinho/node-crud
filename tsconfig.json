{
  "include": [
    "src/**/*"
, "src/database"  ],
   "compilerOptions": {
      "lib": [
         "es5",
         "es6"
      ],
      "target": "ES2021",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./build",
      "emitDecoratorMetadata": true,
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "forceConsistentCasingInFileNames": true,
      "sourceMap": true,
      "strict": true,
      "skipLibCheck": true
   }
}